<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="grdpinfotech.ganeshsatkar.saptsurmusic">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission
        android:name="android.permission.READ_PHONE_STATE"
        tools:node="remove" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission
        android:name="android.permission.GET_ACCOUNTS"
        tools:node="remove" />
    <uses-permission
        android:name="android.permission.READ_PROFILE"
        tools:node="remove" />
    <uses-permission
        android:name="android.permission.READ_CONTACTS"
        tools:node="remove" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo1"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <!-- <activity android:name=".Designby" android:parentActivityName=".MainActivity" android:label="Design By"/> -->
        <activity
            android:name=".Menu"
            android:label="Enquiry"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Feedback"
            android:label="Feedback"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Aboutus"
            android:label="About Us"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Contact"
            android:label="Contact Us"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Keyperson"
            android:label="Key Person"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Artist"
            android:label="Artist"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Designby"
            android:label="Design By"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Upcoming"
            android:label="Upcoming Music"
            android:parentActivityName=".MainActivity"
            tools:ignore="DuplicateActivity" />
        <activity
        android:name=".Gallery"
        android:label="Gallery"
        android:parentActivityName=".MainActivity"
        tools:ignore="DuplicateActivity" />

        <activity
            android:name=".Music"
            android:label="Music"
            android:parentActivityName=".MainActivity"
            tools:ignore="DuplicateActivity" />

        <activity android:name=".Moreimage"
            tools:ignore="ExtraText">
            android:label="More Images"
            android:parentActivityName=".Gallery"
            tools:ignore="DuplicateActivity">

        </activity>

        <activity android:name=".Video"
            android:label="Video"
            android:parentActivityName=".MainActivity"
            tools:ignore="DuplicateActivity"> </activity>
    </application>

</manifest> <!-- \ android:theme="@style/SplashTheme" -->
